<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_property_bazaar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.rudolph.propertybazaar.PropertyBazaarActivity">
    
    
    <android.support.design.widget.BottomNavigationView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/bniv_bottom_nav"
        android:layout_alignParentBottom="true"
        app:itemBackground="@color/teal"
        app:itemIconTint="@drawable/bnv_tab_item_selector"
        app:itemTextColor="@drawable/bnv_tab_item_selector"
        app:menu="@menu/bottom_nav" />


</RelativeLayout>
